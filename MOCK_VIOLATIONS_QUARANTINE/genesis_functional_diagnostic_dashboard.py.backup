        print("Install with: pip install PyQt5")
        return 1
    
    # Get workspace path
    workspace_path = os.getcwd()
    if len(sys.argv) > 1:
        workspace_path = sys.argv[1]
    
    print("üìä Starting GENESIS Functional Diagnostic Dashboard v2.0")
    print(f"Workspace: {workspace_path}")
    
    app = QApplication(sys.argv)
    app.setApplicationName("GENESIS Functional Diagnostic Dashboard")
    app.setApplicationVersion("2.0")
    
    # Create and show main window
    main_window = GenesisFunctionalDashboard(workspace_path)
    main_window.show()
    
    print("‚úÖ Dashboard launched successfully")
    
    return app.exec_()

if __name__ == "__main__":
