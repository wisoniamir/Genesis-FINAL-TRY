        try:
            if not mt5.initialize():
                logger.error("âŒ MT5 initialization failed")
                self.execution_metrics["mt5_connected"] = False
                return
            
            account_info = mt5.account_info()
            if account_info is None:
                logger.error("âŒ MT5 account info retrieval failed")
                self.execution_metrics["mt5_connected"] = False
                return
            
            self.execution_metrics["mt5_connected"] = True
            logger.info(f"âœ… MT5 connected - Account: {account_info.login}")
            
        except Exception as e:
            logger.error(f"âŒ MT5 connection error: {e}")
            self.execution_metrics["mt5_connected"] = False
    
