{
  "version": "v6.2.0-institutional",
  "real_time_monitoring": true,
  "metrics": {
    "trade_execution_latency": {
      "enabled": true,
      "threshold_ms": 100
    },
    "mt5_connection_health": {
      "enabled": true,
      "check_interval_s": 30
    },
    "eventbus_throughput": {
      "enabled": true,
      "alert_threshold": 1000
    },
    "memory_usage": {
      "enabled": true,
      "alert_threshold_mb": 512
    }
  }
}