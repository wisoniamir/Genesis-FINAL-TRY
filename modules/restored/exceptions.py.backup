        format_dict = {
            "message": self.message,
            "path": "[" + "][".join(repr(k) for k in self.path) + "]",
            "notes": "\n".join(self.notes),
        }
        return "{message}\n\nPath To Error: {path}\n\n{notes}".format(**format_dict)


